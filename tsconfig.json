{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    // "declaration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "outDir": "dist",
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
